<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="model.Users" table="USERS">
        <id name="uid" type="int">
            <generator class="native"/>
        </id>
        <property name="name"   type="java.lang.String" />
        <property name="studentID"   type="java.lang.String" />
        <property name="password"   type="java.lang.String" />
 		<property name="gender"   type="java.lang.String" />
 		<property name="brithday"   type="java.lang.String" />
 		<property name="grade"   type="java.lang.String" />
 		<property name="department"   type="java.lang.String" />
 		<property name="major"   type="java.lang.String" />
 		<property name="email"   type="java.lang.String" />
 		<property name="address"   type="java.lang.String" />
 		<property name="hobby"   type="java.lang.String" />
 		
    </class>
</hibernate-mapping>